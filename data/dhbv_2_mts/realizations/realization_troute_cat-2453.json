{
  "global": {
    "formulations": [
      {
        "name": "bmi_python",
        "params": {
          "python_type": "dhbv2.mts_bmi.MtsDeltaModelBmi",
          "model_type_name": "dhbv2.0 mts",
          "init_config": "./data/dhbv_2_mts/config/bmi_cat-2453.yaml",
          "allow_exceed_end_time": true,
          "main_output_variable": "land_surface_water__runoff_volume_flux",
          "fixed_time_step": true,
          "uses_forcing_file": false,
          "variables_names_map" : {
            "atmosphere_water__liquid_equivalent_precipitation_rate":"precip_rate",
            "land_surface_air__temperature":"TMP_2maboveground",
            "water_potential_evaporation_flux":"PET_hargreaves",
            "land_surface_water__runoff_volume_flux":"streamflow_cms"
          }
        }
      }
    ],
    "forcing": {
      "file_pattern": ".*{{id}}.*.csv",
      "path": "./data/forcing/",
      "provider": "CsvPerFeature"
    }
  },
  "time": {
    "start_time": "2008-01-09 00:00:00",
    "end_time": "2009-01-30 23:00:00",
    "output_interval": 3600
  },
  "routing":{
    "t_route_config_file_with_path": "./data/dhbv_2_mts/config/routing_config.yaml"
  },
  "output_root": "./output/"
}
